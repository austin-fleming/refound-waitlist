{
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
        "source.fixAll.prettier": true
    },
    "cSpell.words": [
        "Openlogin",
        "Postgrest",
        "sonarjs",
        "SUPABASE",
        "usecases",
        "WHITELABEL"
    ],
    "[sql]": {
        "editor.defaultFormatter": "bradymholt.pgformatter"
    },
}